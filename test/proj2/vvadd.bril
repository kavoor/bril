  c: int = add b size;

  i: int = const 0;
sw_loop:
  ai: int = add a i;
  bi: int = add b i;
  ci: int = add c i;

  sw i ai;
  sw i bi;
  sw zero ci;

  i: int = add i inc;
  done: bool = eq i size;
  br done sw_done sw_loop;
sw_done:
  i: int = const 0;

vvadd_loop:
  ai: int = add a i;
  bi: int = add b i;
  ci: int = add c i;

  va: int = lw ai;
  vb: int = lw bi;
  vc: int = add va vb;
  sw vc ci;

  i: int = add i vecSize;
  done: bool = ge i size;
  br done vvadd_done vvadd_loop;
vvadd_done:

  i: int = const 0;
lw_loop:
  ci: int = add c i;
  v: int = lw ci;
  print v;
  i: int = add i inc;
  done: bool = eq i size;
  br done lw_done lw_loop;
lw_done:
